	<mapper namespace="com.example.demo.mapper.UserMapper">
		<insert id="insertUser" parameterType="com.example.demo.vo.UserVO">
			INSERT INTO users(usernames,password,firstName,lastName,age,salary)
			VALUES(#{usernames},#{password},#{firstName},#{lastName},#{age},#{salary})
		</insert>
		
	</mapper>